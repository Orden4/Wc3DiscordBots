{
  "Token": "",
  "JassBotUri": "https://lep.duckdns.org/jassbot/doc/",
  "JassUris": [
    "https://raw.githubusercontent.com/lep/jassdoc/refs/heads/master/common.j",
    "https://raw.githubusercontent.com/lep/jassdoc/refs/heads/master/Blizzard.j",
    "https://raw.githubusercontent.com/lep/jassdoc/refs/heads/master/common.ai"
  ],
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "log_.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
